{
    "compilerOptions": {
      "esModuleInterop": true, 
      "forceConsistentCasingInFileNames": true,  
      "module":"es2015",
      "moduleResolution":"node",
      "noEmit": true,
      "preserveConstEnums": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "sourceMap": false,
      "strict": true,
      "target": "es2020",
    },
    "exclude": ["node_modules", "**/*.test.ts"]
  }